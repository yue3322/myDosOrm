        </form>


    </div>
    <div class="demo-block demo-box demo-zh-CN demo-table">
        <div id="dr" class="el-table el-table--fit el-table--border el-table--enable-row-hover el-table--enable-row-transition" style="width: 100%;">

        </div>
    </div>
</body>
</html>
<script>
    function loadData() {
        var id = $("#dehid").val();
        var customercd = $("#customercd").val();
        if (id == null || id == "" || customercd == null || customercd == "") {
            alert("请填写完整");
            return;
        }
        $.post("/home/DelDeh", { id: id, customercd: customercd }, function (data) {
            if (data.Success) {
                alert("删除成功");
            } else {
                alert(data.Message);
            }
        });
    }

</script>

